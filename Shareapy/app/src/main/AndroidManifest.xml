    package="com.example.shareapy">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_mini"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".authentication.ForgotPasswordActivity"></activity>
        <activity android:name=".home.community.CommunityEditPostActivity" />
        <activity android:name=".home.community.CommunityNewPostActivity" />
        <activity android:name=".authentication.SignUpActivity" />
        <activity android:name=".authentication.SignUpSlides" />
        <activity android:name=".authentication.LoginFeeling" />
        <activity android:name=".authentication.LoginActivity" />
        <activity android:name=".home.Home" />
        <activity android:name=".authentication.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".home.MainActivity" />
    </application>

</manifest>